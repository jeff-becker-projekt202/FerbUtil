    "homepage": "https://github.com/jeff-becker-projekt202/FerbUtil",
    "license": "MIT",
    "require": {

    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.8",
        "friendsofphp/php-cs-fixer": "^3.0.2"
    },
    "autoload": {
        "psr-4": {
